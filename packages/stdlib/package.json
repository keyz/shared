    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/react": "^19.0.2",
    "react": "^19.0.0",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keyz/shared.git",
    "directory": "packages/stdlib"
  }
}
