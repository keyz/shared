{
  "name": "@keyanz/stdlib",
  "version": "0.12.0",
  "license": "MIT",
  "exports": {
    "./*": "./dist/public/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/public/*"
      ]
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
